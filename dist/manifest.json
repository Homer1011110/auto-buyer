{
  "manifest_version": 2,
  "name": "auto buyer",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": {
      "16": "imgs/auto-buyer.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://miao.item.taobao.com/*",
        "*://item.jd.com/*",
        "*://cashier.jd.com/*"
      ],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ]
}
